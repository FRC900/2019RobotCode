<?xml version="1.0"?>
<launch>
	<param name="use_sim_time" value="true" />

	<node pkg="rosbag" type="play" name="player" output="screen" args="--clock -l /home/ubuntu/120_cm_high_angled.bag /zed_goal/depth/depth_registered:=/zed_data/goal_detection_node/zed_goal/depth/depth_registered /zed_goal/left/image_rect_color:=/zed_data/goal_detection_node/zed_goal/left/image_rect_color"/>

	<node pkg="tf2_ros" type="static_transform_publisher" name="base_link_to_zed" args="kds -0.072263 0.0244348 0 0 0 base_link zed_camera_center" />

	<node pkg="tf2_ros" type="static_transform_publisher" name="base_link_to_panel_outtake" args="-0.074422 -0.403098 0 0 0 0 base_link panel_outtake" />
</launch>

